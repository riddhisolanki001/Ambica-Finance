 "html": "<html>\n   <body>\n      <table class=\"first-table\" style=\"width:100%; border:2px solid black; border-bottom:1px solid black;\">\n         <tr>\n            <td colspan=\"6\" style=\"text-align: center; font-size:14px; font-weight:bold;\">EXPORT INVOICE</td>\n         </tr>\n         <tr>\n            <td colspan=\"2\" rowspan=\"4\" style=\"width:50%\">\n               <p>{% set doc_company = frappe.get_doc('Company', doc.company) %}\n                  {{ doc_company.name }}\n               </p>\n               <p>{% set doc_address = frappe.get_doc('Address', doc.company_address) %}\n                  {{ doc_address.address_line1 or \"\" }},{{ doc_address.address_line2 or \"\"}} ,<br>\n                  {{ doc_address.city or \"\"}},{{ doc_address.state or \"\" }},{{doc_address.country or \"\"}},{{ doc_address.pincode or \"\"}},\n               </p>\n               <p>{% set doc_address = frappe.get_doc('Address', doc.company_address) %}\n                  {{ doc_address.address_line1 or \"\" }},{{ doc_address.address_line2 or \"\"}} ,<br>\n                  {{ doc_address.city or \"\"}},{{ doc_address.state or \"\" }},{{doc_address.country or \"\"}},{{ doc_address.pincode or \"\"}},\n               </p>\n            </td>\n            <td colspan=\"2\" style=\"width:50%\">\n               <p>Invoice No & Date:</p>\n               <p>Exporter's Ref:</p>\n               <p>Date:</p>\n               <p>Invoice No & Date:</p>\n            </td>\n         </tr>\n         <tr>\n            <td colspan=\"2\">Buyer's Order No. & Date: </td>\n         </tr>\n         <tr>\n            <td colspan=\"2\">L/C. No:</td>\n         </tr>\n         <tr>\n            <td colspan=\"2\">Other Ref.</td>\n         </tr>\n         <tr>\n            <td colspan=\"2\">\n               Consignee:\n            </td>\n            <td colspan=\"2\">\n               Buyer (if other than consignee)\n            </td>\n         </tr>\n         <tr>\n            <td style=\"text-align:center;\">\n               Pre-Carriage by\n            </td>\n            <td style=\"text-align:center;\">\n               Place of receipt by  Pre-Carriage\n            </td>\n            <td style=\"width:15%;text-align:center;\">\n               Country of Origin of Goods\n            </td>\n            <td style=\"text-align:center;\">\n               Country of Final Destination\n            </td>\n         </tr>\n         <tr>\n            <td style=\"text-align:center;\">\n               Vessel/Flight No.\n            </td>\n            <td style=\"text-align:center;\">\n               Port of Loading\n            </td>\n            <td colspan=\"2\" rowspan=\"2\" style=\"text-align:center;\">\n               Terms of Delivery and Payment\n            </td>\n         </tr>\n         <tr style=\"border-bottom:0;\">\n            <td style=\"width:25%; text-align:center;\">\n               Port of Discharge\n            </td>\n            <td style=\"width:25%; text-align:center;\">\n               Final Destination\n            </td>\n         </tr>\n      </table>\n                <table class=\"second-table\" style=\"width:100%; border:2px solid black; border-top:0px; border-bottom:0px;\">\n                  <tr>\n                     <td colspan=\"3\" style=\"width:65%; text-align:center; font-weight:bold;\">\n                        Marks $ Nos./No. & Kind of Pkgs. Description of Goods\n                     </td>\n                     <td rowspan=\"2\" style=\" widht:10%; text-align:center; font-weight:bold;\">\n                        Quantity\n                     </td>\n                     <td  rowspan=\"2\" style=\" widht:10%; text-align:center; font-weight:bold;\">\n                        Rate\n                     </td>\n                     <td rowspan=\"2\" style=\" widht:15%; text-align:center; font-weight:bold;\">\n                        AMOUNT\n                     </td>\n                  </tr>\n                  <tr>\n                     <td colspan=\"3\"></td>\n                  </tr>\n                  <tr>\n                     <td style=\"width:25%; font-weight:bold; text-align:center;\">\n                       TYPE\n                     </td>\n                     <td style=\"font-weight:bold; text-align:center;\">\n                       SIZE(METER)\n                     </td>\n                     <td style=\"font-weight:bold; text-align:center;\">\n                       LENGTH PER PC. (METER)\n                     </td>\n                     <td style=\"font-weight:bold; text-align:center;\">\n                       ROLL\n                     </td>\n                     <td style=\"font-weight:bold; text-align:center;\">\n                       US $ <br> FOB PER ROLL\n                     </td>\n                     <td>\n                       \n                     </td>\n                  </tr>\n                  {% for item in doc.items %}\n                  <tr>\n                     <td>{{ item.item_code }} {{item.item_name}}</td>\n                     <td>{{ item.uom }}</td>\n                     <td>*   {{item.qty}}</td>\n                     <td>{{item.qty}}</td>\n                     <td>{{item.rate}}</td>\n                     <td>{{item.amount}}</td>\n                  </tr>\n                  {% endfor %}\n                  <!--<tr>-->\n                  <!--<td colspan=\"6\"></td>-->\n                  <!--</tr>-->\n                <tr>\n                     <td colspan=\"3\">\n                        <p style=\"font-weight:bold\">Total : </p>\n                     </td>\n                     <td>\n                        <p style=\"font-weight:bold\">{{doc.total_qty}}</p>\n                     </td>\n                     <td>\n                        <p></p>\n                     </td>\n                     <td>\n                        <p style=\"font-weight:bold\">{{doc.total}}</p>\n                     </td>\n                  </tr>\n                                    <tr>\n            <td colspan=\"6\" style=\"font-weight:bold;\">Amount (In Words) US$: {{doc.in_words}} </td>\n         </tr>\n                  <tr>\n                     <td colspan=\"6\">\n                        <p>C.EX.SR.NO : </p>\n                        <p>LINER SEAL NO : </p>\n                        <p>CONTAINER NO & SIZE : </p>\n                        <p>TRUCK NO : </p>\n                     </td>\n                  </tr>\n\n                  <!--<tr></tr>-->\n\n                  <tr>\n            <td colspan=\"6\" style=\"text-align: center;font-weight:bold;\">GROSS WT. :  <br> NET WT. :  </td>\n         </tr>\n\n               </table>\n               <table class=\"third-table\" style=\"width:100%; border:2px solid black; border-top:0px;\">\n                  <tr style=\"border-top:0;\">\n            <td colspan=\"3\" style=\" width:50%;font-weight:bold;\">\n                <p style=\"margin-bottom:65px;\">Signature For</p>\n                <p style=\"text-align:center;\">Authorised Signatory</p>\n            </td>\n            <td colspan=\"3\" style=\" text-align: center;font-weight:bold;\">  </td>\n         </tr>\n               </table>\n\n   </body>\n</html>\n\n\n\n\n <!--{% for item in doc.items %}-->\n <!--           <tr height=18px style=\"text-align:center;\">-->\n <!--               <td style=\" text-align:center\">{{ item.idx }}</td>-->\n <!--                   <td style=\" text-align:left\"><b>{{ item.item_name }}</b>-->\n <!--                 <br><p style=\"font-size:11px;margin-left:10px; text-align:left\">{{item.description or \" \" }}</p> </td>-->\n                 \n <!--                 <td style=\" text-align:center\">{{ item.gst_hsn_code }}</td>-->\n <!--                 <td style=\" text-align:right\">{{ '{:.3f}'.format(item.qty) }}</td>-->\n <!--                 <td style=\" text-align:center\">{{ item.uom }}</td>-->\n <!--                 <td style=\" text-align:right\">{{item.get_formatted(\"rate\")}}</td>-->\n               \n <!--                 <td style=\" text-align:right\">{{item.get_formatted(\"amount\")}}</td>-->\n <!--           </tr>-->\n <!--           {% endfor %}-->\n \n \n \n \n \n \n ",
